{
  "roots": [
    "<rootDir>/test"
  ],
  "clearMocks": true,
  "collectCoverageFrom": [
    "src/**/{!(event-job-data),}.ts"
  ],
  "collectCoverage": true,
  "coverageDirectory": "coverage",
  "coverageProvider": "v8",
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "function": 90,
      "lines": 90,
      "statements": 90
    }
  },
  "transform": {
    ".+\\.ts$": "ts-jest"
  },
  "testTimeout": 120000
}
